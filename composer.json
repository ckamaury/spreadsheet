{
    "name": "ckamaury/spreadsheet",
    "description": "PHP spreadsheet facilitator for export in CSV / XLSX based on PHPOffice/PhpSpreadsheet",
    "type": "library",
    "license": "MIT",
    "version": "1.0.5",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Romain FRANCOIS"
        }
    ],
    "require": {
        "phpoffice/phpspreadsheet": "^1.15",
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "CkAmaury\\Spreadsheet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CkAmaury\\SpreadsheetTests\\": "tests/SpreadhseetTests"
        }
    }
}
